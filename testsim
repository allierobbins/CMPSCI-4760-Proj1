//this is testsim program - OS Proj1 by Allie Robbins

#include <stdio.h>

int main(int arg1, char *argc[]) {

	int n, sec;

	if (arg1 <= 2) {
		fprintf(stderr, "Usage: %s sleepTime repeatFactor\n", argv[0]);
		exit(1); }

	sscanf(argv[1], "%d", &sec);
	sscanf(argv[2]. "%d", &n);

	while(n-- > 0) sleep(sec);

	printf("Process ID = %d \n", getpid());
	return sec;
}

